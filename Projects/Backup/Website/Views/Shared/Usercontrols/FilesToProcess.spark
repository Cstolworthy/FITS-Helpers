<viewdata model="Interfaces.Website.Model.IProcessModel" />
<div>
  <label for="fileNames">Select a File: </label>
  <select id="fileNames" name="fileNames">
    <for each="var file in ViewData.Model.Files">
      <option value="${file.Key}">${file.Key}</option>
    </for>
  </select>
  <input type="button" value="Select" onclick="selectFile();" />
</div>